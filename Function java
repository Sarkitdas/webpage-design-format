<html>
    <head>
        <title>Sarkit</title>
    </head>
    <body>
        <script>
            function java()
            {
            var n=prompt("Enter the number: ");
            var m=n*n;
            document.write("Result is: "+m+ "<br>");
            }
            java();
            java();

            //For different value passing into the perimeter
        
            function java2(n,m)
            {
            var m=n*m;
            document.write("Result is: " +m+ "<br>");
            }
            java2(5,2);

            //returning in a function

            function java3(n,m)
            {
            var m=n/m;
            return m;
            }
            document.write("Result is: "+(java3(6,2)));

        </script>
    </body>
</html>
