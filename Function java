        <script>
            function java()
            {
            var n=prompt("Enter the number: ");
            var m=n*n;
            document.write("Result is: "+m);
            }
            java();
            java();

            //For different value passing into the perimeter
        
            function java2(n,m)
            {
            var m=n*m;
            document.write("Result is: "+m);
            }
            java2(5,2);
        </script>
    </body>
